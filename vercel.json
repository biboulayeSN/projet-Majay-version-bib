{
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/admin/:path*"
    },
    {
      "source": "/vendeur/:path*",
      "destination": "/vendeur/:path*"
    },
    {
      "source": "/catalogue.html",
      "destination": "/catalogue.html"
    },
    {
      "source": "/:slug",
      "destination": "/store-router.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
      },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

